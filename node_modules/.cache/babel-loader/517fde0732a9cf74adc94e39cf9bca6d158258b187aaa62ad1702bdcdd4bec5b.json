bled=\"!someSelected\"\r\n        title=\"de-select all [ctrl + shift + a]\"\r\n        :class=\"{'buttons' : someSelected, 'disabledButton' : !someSelected}\"\r\n      >\r\n        deselect all\r\n      </button>\r\n      <div class=\"username\" v-html=\"'welcome,&nbsp;&nbsp;&nbsp;<br>' + state.username\"></div>\r\n    </div>\r\n    <div v-else class=\"toolbarSection\">\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport User from './User'\r\n\r\nexport default {\r\n  name: 'Toolbar',\r\n  props: [ 'state' ],\r\n  components: {\r\n    User\r\n  },\r\n  data(){\r\n    return {\r\n    }\r\n  },\r\n  computed: {\r\n    someSelected(){\r\n      return this.state.links.filter(v=>v.selected).length || this.state.userLinks.filter(v=>v.selected).length\r\n    },\r\n    allSelected(){\r\n      return (this.state.links.filter(v=>v.selected).length + this.state.userLinks.filter(v=>v.selected).length) == this.state.links.length + this.state.userLinks.length\r\n    }\r\n  },\r\n  methods: {\r\n  },\r\n  mounted(){\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n  .toolbar{\r\n    height: 38px;\r\n    background: linear-gradient(90deg, #3336, #3336, #3333);\r\n    color: #4fa;\r\n    font-size: 20px;\r\n    position: absolute;\r\n    top: 52px;\r\n    z-index: 100;\r\n    width: calc(100vw - 20px);\r\n  }\r\n  .toolbarSection{\r\n    display: inline-block;\r\n    border-left: 8px solid #40fa;\r\n  }\r\n  .toolbarButtons{\r\n    margin: 5px;\r\n    min-width: 100px;\r\n    padding: 2px;\r\n    padding-bottom: 0;\r\n    border-radius: 5px;\r\n  }\r\n  .username{\r\n    display: inline-block;\r\n    position: absolute;\r\n    text-align: right;\r\n    right: 5px;\r\n  }\r\n  .deleteButton{\r\n    color: #f88;\r\n    background-color: #400d;\r\n    border-color: #8008;\r\n  }\r\n  .disabledButton{\r\n    color: #888;\r\n    background-color: #333d;\r\n    border-color: #0008;\r\n  }\r\n</style>\r\n"],"mappings":"AA6CA,OAAOA,IAAG,MAAO,QAAO;AAExB,eAAe;EACbC,IAAI,EAAE,